{
  "name": "@danielnaranjo/angular-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "ng serve --port=4100 --live-reload -o",
    "dev:secure": "ng serve --ssl true  --ssl-cert ssl/localhost.crt --ssl-key \"ssl/localhost.key\" --open=true --port=4100 --host=0.0.0.0 --disable-host-check",
    "dev:ssr": "ng run angular-boilerplate:serve-ssr",
    "serve:ssr": "node dist/angular-boilerplate/server/main.js",
    "build:ssr": "ng build --configuration production && ng run angular-boilerplate:server:production",
    "prerender": "ng run angular-boilerplate:prerender",
    "dev:proxy": "ng serve --port=4100 --live-reload --proxy-config src/proxy.conf.json -o"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/platform-server": "~13.3.11",
    "@angular/router": "~13.3.11",
    "@nguniversal/express-engine": "^13.1.1",
    "@ngx-formly/bootstrap": "^5.0.0",
    "@ngx-formly/core": "^5.0.0",
    "@sentry/browser": "^5.27.6",
    "@sentry/tracing": "^5.27.6",
    "@types/crypto-js": "^3.1.47",
    "crypto-js": "3.1.9-1",
    "express": "^4.15.2",
    "moment": "^2.29.1",
    "onscreen": "1.3.4",
    "popper.js": "1.16.0",
    "prismjs": "1.17.1",
    "rxjs": "~6.6.0",
    "smooth-scroll": "^16.1.0",
    "tslib": "^2.0.0",
    "waypoints": "4.0.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.9",
    "@angular/cli": "~13.3.9",
    "@angular/compiler-cli": "~13.3.11",
    "@nguniversal/builders": "^13.1.1",
    "@ngx-formly/schematics": "^5.10.29",
    "@types/chart.js": "^2.9.34",
    "@types/express": "^4.17.0",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.12",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngc-esbuild": "0.0.60",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  },
  "browser": {
    "crypto": false
  }
}
